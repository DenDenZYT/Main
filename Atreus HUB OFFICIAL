local gui = Instance.new("ScreenGui")
gui.Name = "TinyUI"
gui.DisplayOrder = 10
gui.Parent = game.Players.LocalPlayer.PlayerGui

local officialKeys = {
    "KEY-VIOLAFOUNTAIN",
    "KEY-ANONYMOUSCHEETAH",
    "KEY-SLICKROACH"
}

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 200, 0, 100)
frame.Position = UDim2.new(0.5, -100, 0.5, -50)
frame.AnchorPoint = Vector2.new(0.5, 0.5)
frame.BackgroundTransparency = 0.5
frame.BackgroundColor3 = Color3.new(139 / 255, 0, 0)
frame.Parent = gui

local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 20)
title.Position = UDim2.new(0, 0, 0, 0)
title.Text = "Atreus HUB KEY SYSTEM"
title.TextColor3 = Color3.new(0, 0, 1)
title.Font = Enum.Font.SourceSansBold
title.TextScaled = true
title.Parent = frame

local textbox = Instance.new("TextBox")
textbox.Size = UDim2.new(0.8, 0, 0.2, 0)
textbox.Position = UDim2.new(0.1, 0, 0.4, 0)
textbox.PlaceholderText = "Enter key..."
textbox.TextColor3 = Color3.new(0, 0, 0)
textbox.Parent = frame

local getKeyButton = Instance.new("TextButton")
getKeyButton.Size = UDim2.new(0.8, 0, 0.2, 0)
getKeyButton.Position = UDim2.new(0.1, 0, 0.7, 0)
getKeyButton.Text = "Get Key"
getKeyButton.BackgroundColor3 = Color3.new(1, 0, 0)
getKeyButton.Parent = frame

local enterKeyButton = Instance.new("TextButton")
enterKeyButton.Size = UDim2.new(0.2, 0, 0.2, 0)
enterKeyButton.Position = UDim2.new(0.75, 0, 0.7, 0)
enterKeyButton.Text = "Enter"
enterKeyButton.BackgroundColor3 = Color3.new(0, 1, 0)
enterKeyButton.Parent = frame

getKeyButton.MouseButton1Click:Connect(function()
    print("Get Key clicked")
    setclipboard("https://link-target.net/966464/key-for-dendenz-hub")
end)

enterKeyButton.MouseButton1Click:Connect(function()
    print("Enter Key clicked")
    local enteredKey = textbox.Text
    if table.find(officialKeys, enteredKey) then
        print("Entered Key is valid. Executing the script...")
        gui:Destroy()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/DenDenZYT/Main/main/ATREUS%20HUB"))()
    else
        print("Invalid Key.")
    end
end)
