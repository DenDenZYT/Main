if _G.Mode == "Orion" then
    loadstring(game:HttpGet('https://raw.githubusercontent.com/DenDenZYT/ScriptCollectionKeyless/main/ScriptsV2'))()
elseif _G.Mode == "Ray" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/DenDenZYT/ScriptCollectionKeyless/main/Script%20Collection%20V2%20(DR%20RAY%20LIBRARY)"))()
elseif _G.Mode == "Ray2" then
local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("DenDenZ Script Collection", "UPDATE 1.1.1.")

--[[Bedwars]] function vapebedwars()
    loadstring(
        game:HttpGet("https://raw.githubusercontent.com/7GrandDadPGN/VapeV4ForRoblox/main/NewMainScript.lua", true)
    )()
    window:Close()
end
 --

--[[Haze Piece]] function hyperhazepiece()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/DookDekDEE/All-Script/main/hp.lua"))()
    window:Close()
end
 --

--[[Your Bizarre Adventure]] function ybacuzyesyourbizzareadventure()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NukeVsCity/hackscript123/main/gui"))()
    window:Close()
end

function ybasucksassyourbizzareadventure()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Tobias020108Back/YBA-AUT/main/YBA-GUI-Rewrite.lua"))()
    window:Close()
end

function bitchboyyourbizzareadventure()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/zakater5/LuaRepo/main/YBA/v3.lua"))()
    window:Close()
end


local bedwarstab = DrRayLibrary.newTab("Bedwars", nil)
local hazepiecetab = DrRayLibrary.newTab("HazePiece", nil)
local ybatab = DrRayLibrary.newTab("YBA", nil)
 --

-- Buttons

--[[Bedwars]] bedwarstab.newButton(
    "Vape V4",
    "Keyless : ✅ Status : Working",
    function()
        vapebedwars()
    end
)
 --

--[[Haze Piece]] hazepiecetab.newButton(
    "Hyper Hub",
    "Keyless : ❌ Status : Working",
    function()
        hyperhazepiece()
    end
)
 --

--[[Your Bizzare Adventure]] ybatab.newButton(
    "YBA cuz Yes",
    "Keyless : ✅ Status : Working",
    function()
        ybacuzyesyourbizzareadventure()
    end
)

ybatab.newButton(
    "YBA sucks ass",
    "Keyless : ✅ Status : Working",
    function()
        ybasucksassyourbizzareadventure()
    end
)

ybatab.newButton(
    "Bitch Boy V3",
    "Keyless : ✅ Status : Working",
    function()
        bitchboyyourbizzareadventure()
    end
  )

end
